<script>
const hlTextAnimation = setInterval(() => {
    let content = $('#hl-animated-text').text();
    switch (content) {
        case 'Anticipated':
            $('#hl-animated-text').text('Innovative');
            break;
        case 'Innovative':
            $('#hl-animated-text').text('Inimitable');
            break;
        case 'Inimitable':
            $('#hl-animated-text').text('Anticipated');
            break;
    }
}, 1000);
</script>
